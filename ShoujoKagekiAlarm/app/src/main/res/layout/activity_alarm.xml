<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".alarm">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="match_parent"
                android:layout_height="160dp"
                android:layout_marginTop="50dp"
                android:contentDescription="@+string/title"
                app:srcCompat="@drawable/title" />

            <ImageView
                android:id="@+id/a1"
                android:layout_width="match_parent"
                android:layout_height="72dp"
                android:layout_marginTop="-60dp"
                android:contentDescription="@+string/a1"
                android:visibility="gone"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/a1" />

            <ImageView
                android:id="@+id/a2"
                android:layout_width="match_parent"
                android:layout_height="72dp"
                android:layout_marginTop="-70dp"
                android:contentDescription="@+string/a2"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/a1"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/a1"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/a2" />

            <ImageView
                android:id="@+id/a3"
                android:layout_width="match_parent"
                android:layout_height="72dp"
                android:layout_marginTop="-72dp"
                android:contentDescription="@+string/a3"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/a2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/a2"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/a3" />

            <ImageView
                android:id="@+id/a4"
                android:layout_width="match_parent"
                android:layout_height="72dp"
                android:layout_marginTop="-75dp"
                android:contentDescription="@+string/a4"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/a3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/a3"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/a4" />

            <ImageView
                android:id="@+id/a5"
                android:layout_width="match_parent"
                android:layout_height="72dp"
                android:layout_marginTop="-72dp"
                android:contentDescription="@+string/a5"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/a4"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/a4"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/a5" />

            <ImageView
                android:id="@+id/a6"
                android:layout_width="match_parent"
                android:layout_height="72dp"
                android:layout_marginTop="-72dp"
                android:contentDescription="@+string/a6"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/a5"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/a5"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/a6" />

            <ImageView
                android:id="@+id/a7"
                android:layout_width="match_parent"
                android:layout_height="72dp"
                android:layout_marginTop="-70dp"
                android:contentDescription="@+string/a7"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/a6"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/a6"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/a7" />

            <ImageView
                android:id="@+id/a8"
                android:layout_width="match_parent"
                android:layout_height="72dp"
                android:layout_marginTop="-70dp"
                android:contentDescription="@+string/a8"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/a7"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/a7"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/a8" />

            <ImageView
                android:id="@+id/a9"
                android:layout_width="match_parent"
                android:layout_height="72dp"
                android:layout_marginTop="-72dp"
                android:contentDescription="@+string/a9"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/a8"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/a8"
                app:layout_constraintVertical_bias="0.0"
                app:srcCompat="@drawable/a9" />

            <ImageView
                android:id="@+id/giraffeIcon"
                android:layout_width="170dp"
                android:layout_height="170dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="30dp"
                android:contentDescription="@+string/icon"
                app:srcCompat="@drawable/icon" />

            <ImageView
                android:id="@+id/imageView8"
                android:layout_width="match_parent"
                android:layout_height="156dp"
                android:layout_marginTop="20dp"
                android:contentDescription="@+string/content"
                app:srcCompat="@drawable/context" />

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="match_parent"
                android:layout_height="248dp"
                android:layout_marginTop="50dp"
                android:contentDescription="@+string/tower"
                app:srcCompat="@drawable/tower" />

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="match_parent"
                android:layout_height="184dp"
                android:layout_marginTop="50dp"
                android:contentDescription="@+string/notification"
                app:srcCompat="@drawable/notification" />


            <ImageButton
                android:id="@+id/alarmstop"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="50dp"
                android:background="#000000"
                android:contentDescription="@+string/hangup"
                android:scaleType="fitXY"
                app:srcCompat="@drawable/phoneee" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="match_parent"
                android:layout_height="63dp"
                android:layout_marginBottom="20dp"
                android:layout_marginTop="20dp"
                android:contentDescription="@+string/footer"
                app:srcCompat="@drawable/footer" />

        </LinearLayout>
    </ScrollView>

</android.support.constraint.ConstraintLayout>